# Performance Settings
CACHE_ENABLED=true
CACHE_TTL=3600

# Development Settings
LOG_LEVEL=INFO
DEBUG_MODE=false

# Authentication (optional - leave empty to disable)
# AUTHENTIK_CLIENT_ID=
# AUTHENTIK_CLIENT_SECRET=
# AUTHENTIK_ISSUER_URL=

# Docker-specific
UV_CACHE_DIR=/tmp/uv-cache