# Server Configuration
MCP_TRANSPORT=http
MCP_ONLY=false
MCP_HOST=localhost
MCP_PORT=3000
MCP_AUTH_ENABLED=false

# Search Configuration
DUCKDUCKGO_MAX_RESULTS=10
SEARCH_TIMEOUT=30
